 file = {Varsier et al. - 2021 - A 5G New Radio for Balanced and Mixed IoT Use Case.pdf:/Users/adjih/Zotero/storage/MHDV65YR/Varsier et al. - 2021 - A 5G New Radio for Balanced and Mixed IoT Use Case.pdf:application/pdf},
