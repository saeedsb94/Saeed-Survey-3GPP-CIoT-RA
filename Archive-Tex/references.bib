@inproceedings{damjancevic_flexible_2021,
 abstract = {The new 5G Reduced Capability (RedCap) protocol offers up to 88x and 528x higher data rates and dynamic pilot placements compared to previous Cat-M and NB-IoT standards, respectively. This leads to high application variability of IoT devices and therefore poses a challenge for the implementation of channel estimation (CE), especially under weak radio signal conditions. However, due to the computational complexity of optimal methods, practical suboptimal approaches with denoising capability are preferred in low-power devices. This work investigates the performance and implementation aspects of practical IoT CE denoising techniques on a vector digital signal processor (vDSP). This solution enables adaptation to the new IoT workload requirements with a 15.9x speed-up compared to the non-vectorised approach at 99.2\% processor efficiency. In addition, for the purpose of solution adaptation to various IoT standards, the clock frequency requirements for the complete channel estimation chain are analysed with respect to different processor configurations.},
 author = {Damjancevic, Stefan A. and Dasgupta, Samuel Ajay and Matus, Emil and Utyanksy, Dmitry and van der Wolf, Pieter and Fettweis, Gerhard P.},
 booktitle = {2021 {IEEE} {Workshop} on {Signal} {Processing} {Systems} ({SiPS})},
 doi = {10.1109/SiPS52927.2021.00011},
 file = {IEEE Xplore Abstract Record:/Users/adjih/Zotero/storage/MMDCTSX3/9605060.html:text/html;IEEE Xplore Full Text PDF:/Users/adjih/Zotero/storage/TXFA89LX/Damjancevic et al. - 2021 - Flexible Channel Estimation for 3GPP 5G IoT on a V.pdf:application/pdf},
 keywords = {5G mobile communication, Protocols, Channel estimation, 3GPP, 5G, Signal processing algorithms, Approximation algorithms, IoT, Cat-M, Channel Estimation, Denoising, Digital signal processors, Moving Average, NB-IoT, Noise reduction, RedCap, SIMD, vDSP, VLIW},
 month = {October},
