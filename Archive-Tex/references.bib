 issn = {2471-2833},
 journal = {IEEE Communications Standards Magazine},
 keywords = {Performance evaluation, Wireless communication, Wireless sensor networks, Ultra reliable low latency communication, Power demand, Wearable computers, Enhanced mobile broadband},
 month = {September},
