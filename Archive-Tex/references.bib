 issn = {2327-4662},
 journal = {IEEE Internet of Things Journal},
 keywords = {machine learning, Machine learning, Wireless communication, Internet of Things, 5G mobile communication, Internet of Things (IoT), Security, 6G, 6G mobile communication, blockchain, Blockchain, space–air–ground–underwater networks},
 month = {August},
