@article{zhou_analyzing_2022_4,
 abstract = {Fifth Generation (5G) New Radio (NR) does not support data transmission during random access (RA) procedures, which results in unnecessary control signalling overhead and power consumption, especially for small data transmission (SDT). Motivated by this, 3GPP has proposed 4/2-step SDT RA schemes based on the existing grant-based (4-step) and grant-free (2-step) RA schemes, with the aim to enable data transmission during RA procedures in Radio Resource Control (RRC) Inactive state. To compare the 4/2-step SDT RA schemes with the benchmark 4/2-step RA schemes, we provide a spatio-temporal analytical framework to evaluate the RA schemes, which jointly models the preamble detection, Physical Uplink Shared Channel (PUSCH) decoding, and data transmission procedures. Based on this analytical model, we derive the analytical expressions for the overall packet transmission success probability and average throughput in each RACH attempt. We also derive the average energy consumption in each RACH attempt. Our results show that 2-step SDT RA scheme provides the highest overall packet transmission success probability, and the lowest average energy consumption, but the performance gain decreases with the increase of device intensity.},
 author = {Zhou, Hui and Deng, Yansha and Feltrin, Luca and Höglund, Andreas},
 doi = {10.1109/TCOMM.2022.3150787},
 file = {arXiv Fulltext PDF:/Users/adjih/Zotero/storage/HI3HQUND/Zhou et al. - 2022 - Analyzing Novel Grant-Based and Grant-Free Access .pdf:application/pdf;arXiv.org Snapshot:/Users/adjih/Zotero/storage/DCWSMI3Y/2102.html:text/html},
 issn = {0090-6778, 1558-0857},
 journal = {IEEE Transactions on Communications},
 keywords = {Electrical Engineering and Systems Science - Systems and Control},
 month = {April},
 note = {arXiv:2102.10405 [cs, eess]},
 number = {4},
 pages = {2805--2819},
 title = {Analyzing {Novel} {Grant}-{Based} and {Grant}-{Free} {Access} {Schemes} for {Small} {Data} {Transmission}},
 url = {http://arxiv.org/abs/2102.10405},
 urldate = {2022-06-07},
 volume = {70},
 year = {2022}
}


