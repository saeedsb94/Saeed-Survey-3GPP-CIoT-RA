 keywords = {security, Data privacy, Privacy, Access control, Bibliographies, Case Report Form (CRF), Clinical trials, Distributed databases, Electronic Data Capture (EDC), GDPR, Health Care, Personal Health Train, privacy, Regulation},
 month = {October},
 note = {ISSN: 2325-6605},
 pages = {216--225},
 title = {An information security diagnostic of {Electronic} {Data} {Capture} {Systems} for the {Personal} {Health} {Train}},
